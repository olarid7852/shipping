<?xml version="1.0" encoding="utf-8"?>
<odoo>  <!-- Data records go here -->
  <record id='library_book_action' model='ir.actions.act_window'>
    <field name="name">Aircargo List</field>
    <field name="res_model">library.aircargo</field>
    <!-- <field name="view_type">form</field> -->
    <field name="view_mode">tree,form</field>
  </record>
  <record id='ship_action' model='ir.actions.act_window'>
    <field name="name">Shipping list</field>
    <field name="res_model">library.ship_cargo</field>
    <!-- <field name="view_type">form</field> -->
    <field name="view_mode">tree,form</field>
  </record>
  <record id='aircago_shipment_item' model='ir.actions.act_window'>
    <field name="name">Product</field>
    <field name="res_model">library.aircargo_item</field>
    <!-- <field name="view_type">form</field> -->
    <field name="view_mode">form,tree</field>
  </record>

  <!-- <menuitem name="Aircargo single shipment" id="library_base_menu" action="aircago_shipment_item"/> -->
  <menuitem name="Aircargo Shipments" id="library_book_menu" parent="account.menu_finance" action="library_book_action"/>
  <menuitem name="Ship Shipments" id="library_ship_menu" action="ship_action"/>


  <record id="library_book_view_form" model="ir.ui.view">
    <field name="name">Aircargo Edit</field>
    <field name="model">library.aircargo</field>
    <field name="arch" type="xml">
      <form>
        <group>
          <field name="mawb_no"/>
        </group>
        <group>
          <field name="dischare_port"/>
          <field name="departure_date"/>
          <field name="arrival_date"/>
          <field name="flt_no"/>
          <field name="consign_to"/>
          <field name="shipped_by"/>
        </group>
        <notebook>
          <page string="Items">
            <field name="items"/>
          </page>
        </notebook>
      </form>
    </field>
  </record>

  <record id="library_book_view_tree" model="ir.ui.view">
    <field name="name">Item List</field>
    <field name="model">library.aircargo_item</field>
    <field name="arch" type="xml">
      <tree>
        <field name="hawb_no"/>
        <field name="pkgs"/>
        <field name="wkg"/>
        <field name="vol"/>
        <field name="comodity"/>
        <field name="shipper_id"/>
        <field name="cosignee_id"/>
        <field name="payment_id"/>
        <field name="quantity"/>
        <field name="sign"/>
        <field name="location"/>
        <field name="state"/>
        <field name="customer_order"/>
        <field name="reciept"/>

      </tree>
    </field>
  </record>

  <record id="library_book_ship_tree" model="ir.ui.view">
    <field name="name">Item List</field>
    <field name="model">library.ship_cargo_item</field>
    <field name="arch" type="xml">
      <tree>
        <field name="hbl"/>
        <field name="marks"/>
        <field name="pkg"/>
        <field name="total_cbm"/>
        <field name="weight"/>
        <field name="cosignee_id"/>
        <field name="dest_port"/>
        <field name="amount"/>
        <field name="remark"/>
        <field name="state"/>
      </tree>
    </field>
  </record>

  <record id="library_book_view_tree_1" model="ir.ui.view">
    <field name="name">Aircargo List</field>
    <field name="model">library.aircargo</field>
    <field name="arch" type="xml">
      <tree>
        <field name="mawb_no"/>
        <field name="dischare_port"/>
        <field name="departure_date"/>
        <field name="arrival_date"/>
        <field name="shipped_by"/>
      </tree>
    </field>
  </record>

  <record id="library_book_view_tree_3" model="ir.ui.view">
    <field name="name">Shpping List</field>
    <field name="model">library.ship_cargo</field>
    <field name="arch" type="xml">
      <tree>
        <field name="date"/>
        <field name="remark"/>
      </tree>
    </field>
  </record>

  <!-- <record id="library_book_view_search" model="ir.ui.view">
    <field name="name">Library Book Search</field>
    <field name="model">library.shipment</field>
    <field name="arch" type="xml">
      <search>
        <field name="name"/>
        <field name="author_ids"/>
        <filter string="No Authors" name="without_author" domain="[('author_ids','=',False)]"/>
      </search>
    </field>
  </record> -->
  <record id="library_book_view_form_6" model="ir.ui.view">
    <field name="name">Aircargo Edit</field>
    <field name="model">library.aircargo_item</field>
    <field name="arch" type="xml">
      <form modifiers="{}">
        <header modifiers="{}">
          <button name="button_set_arival" string="Is available" type="object" attrs="{'invisible' : [ ('state', 'not in', ('draft',))]}" modifiers="{'invisible':[['state','not in',['draft']]]}" options="{}"/>
          <field name="state" widget="statusbar" statusbar_visible="draft,posted" on_change="1" modifiers="{'readonly':true,'required':true}"/>
        </header>
        <group modifiers="{}">
          <field name="hawb_no" modifiers="{'required':true}"/>
          <field name="pkgs" modifiers="{'required':true}"/>
          <field name="wkg" modifiers="{'required':true}"/>
          <field name="vol" modifiers="{'required':true}"/>
          <field name="comodity" modifiers="{'required':true}"/>
          <field name="shipper_id" modifiers="{'required':true}"/>
          <field name="cosignee_id" modifiers="{'required':true}"/>
          <field name="payment_id" modifiers="{'required':true}"/>
          <field name="quantity" modifiers="{'required':true}"/>
          <field name="sign" modifiers="{'required':true}"/>
          <field name="location" modifiers="{'required':true}"/>
          <field name="cargo_id" modifiers="{'required':true}"/>
          <field name="reciept" modifiers="{'read_only': true}"/>
          <field name="customer_order" modifiers="{'read_only': true}"/>
        </group>
      </form>
    </field>
  </record>

  <record id="vpicktree_inherit" model="ir.ui.view">
    <field name="name">stock.picking.tree.inherit</field>
    <field name="model">stock.picking</field>
     <field name="inherit_id" ref="stock.vpicktreep"></field>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='state']" position="before">
        <field name="hawb_no"/>
        <field name="shipment_item_id"/>
      </xpath>
    </field>
  </record>
  <record id="vpickform_inherit" model="ir.ui.view">
    <field name="name">stock.picking.form.inherit</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.view_picking_form"></field>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='picking_type_id']" position="before">
        <field name="hawb_no"/>
        <field name="shipment_item_id"/>
      </xpath>
    </field>
  </record>
  
  <record id="invoice_form_inherit" model="ir.ui.view">
    <field name="name">account.move.form.inherit</field>
    <field name="model">account.move</field>
    <field name="inherit_id" ref="account.view_move_form"></field>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='invoice_date']" position="before">
        <field name="hawb_no"/>
        <field name="shipment_item_id"/>
      </xpath>
    </field>
  </record>


  <record id="openacademy_settings_view2" model="ir.ui.view">
    <field name="name">Openacademy Configuration</field>
    <field name="model">res.config.settings</field>
    <field name="inherit_id" ref="base.res_config_settings_view_form" />
    <field name="arch" type="xml">
      <xpath expr="//div[hasclass('settings')]" position="inside">
        <div class="app_settings_block" data-string="Open Academy Data-String" string="Open Academy" data-key="openacademy">
          <h2>General Settings</h2>
          <div class="row mt16 o_settings_container">
            <div class="col-xs-12 col-md-6 o_setting_box">
              <div class="o_setting_right_pane">
                <label for="my_setting"/>
                <div class="text-muted">
                </div>
                <div class="content-group">
                  <div class="mt16">
                    <field name="my_setting" class="o_light_label"/>
                  </div>
                </div>
              </div>
            </div>
            <!-- <div class="col-xs-12 col-md-6 o_setting_box">
              <div class="o_setting_right_pane">
                <label for="default_seats"/>
                <div class="text-muted">
                                    Description text 2
                </div>
                <div class="content-group">
                  <div class="mt16">
                     <field name="my_setting" class="o_light_label"/>
                  </div>
                </div>
              </div> -->
            <!-- </div> -->
          </div>
        </div>
      </xpath>
    </field>
  </record>

  <record id="openacademy_settings_action" model="ir.actions.act_window">
    <field name="name">Openacademy Configuration</field>
    <field name="res_model">res.config.settings</field>
    <field name="view_id" ref="openacademy_settings_view2"/>
    <field name="view_mode">form</field>
    <field name="target">inline</field>
    <field name="context">{'module' : 'library'}</field>
  </record>

  <menuitem id="settings_menu" name="MyModuleConfig" parent="account.menu_finance" action="openacademy_settings_action"/>

</odoo>